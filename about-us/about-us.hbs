{{#each people}}
    <div class="col-lg-6 col-md-6 text-center">
        <div class="mt-5">
            <img src="images/{{imageLink}}" width="235px" height="352px" class="teamimage" alt="{{firstName}} {{lastName}}">
            <h3 class="h4 mb-2">{{firstName}} {{lastName}}</h3>
            <p class="text-muted mb-0">Grade: {{grade}}</p>
            <p class="text-muted mb-0">Hobbies: {{hobbies}}</p>
            <p class="text-muted mb-0">Computer Science Experience: {{csExperience}}.</p>
        </div>
    </div>
{{/each}}
